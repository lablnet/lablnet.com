---
import Layout from "../layouts/Layout.astro";

import Intro from "../components/Home/Intro.astro";
import Contact from "../components/Home/Contact.vue";
import ToggleSwitcher from "../components/Home/ToggleSwitcher.astro";
import Recommendations from "../components/Home/Recommendations.astro";
import { data } from "../utils/const";
---

<Layout title="Muhammad Umer Farooq">
	<!-- Intro -->
	<Intro />

	<!-- Dynamic Sections -->
	{
		data.map((sections: any[]) => {
			return <ToggleSwitcher sections={sections} />;
		})
	}

	<!-- Recommendations -->
	<Recommendations />

	<!-- Contact -->
	<Contact client:only="vue" />
</Layout>
